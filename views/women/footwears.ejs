// Function to Open Quick View
function openQuickView(productId) {
    const product = products.find(p => p.id === productId);

    if (product) {
        document.getElementById("quick-view-image").src = product.image;
        document.getElementById("quick-view-title").innerText = product.name;
        document.getElementById("quick-view-sku").innerText = product.sku;
        document.getElementById("quick-view-old-price").innerText = product.oldPrice;
        document.getElementById("quick-view-price").innerText = product.price;
        document.getElementById("quick-view-description").innerText = product.description;

     
        $('#content_quickview').modal('show');
    }
}


document.querySelectorAll('.quick-view-btn').forEach(button => {
    button.addEventListener('click', () => {
        const productId = parseInt(button.getAttribute('data-product-id'));
        openQuickView(productId);
    });
});

	
